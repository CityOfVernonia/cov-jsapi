{
  "name": "cov-jsapi",
  "version": "0.1.1",
  "description": "City of Vernonia for Esri's JavaScript API.",
  "scripts": {
    "test": "npm run lint && jest",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "prettier": "prettier --single-quote --write \"src/**/*.ts?(x)\"",
    "build": "npx tsc && npx node-sass --output-style compressed src/css/cov.scss > dist/css/cov.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CityOfVernonia/cov-jsapi.git"
  },
  "keywords": [
    "jsapi",
    "widgets",
    "viewmodels",
    "ts"
  ],
  "author": "COV-GIS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CityOfVernonia/cov-jsapi/issues"
  },
  "homepage": "https://github.com/CityOfVernonia/cov-jsapi#readme",
  "devDependencies": {
    "@types/arcgis-js-api": "4.16.0",
    "@types/jest": "^26.0.13",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "arcgis-js-api": "4.16.2",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "jest": "^26.4.2",
    "node-sass": "^4.14.1",
    "prettier": "^2.1.1",
    "ts-jest": "^26.3.0",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2"
  }
}
